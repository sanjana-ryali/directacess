CREATE TABLE IF NOT EXISTS `school_reports`(`id` varchar(255) NOT NULL, `num_parking` INT, `num_handi_parking` INT, `handi_park_image` varchar(255),`if_handi_stripes` BOOLEAN,`if_ext_bleachers` BOOLEAN,`if_wheelchair` BOOLEAN,`if_multi` BOOLEAN,`if_elevator` BOOLEAN,`if_elevator_wide` BOOLEAN,`if_ramp` BOOLEAN,`if_ramp_wide` BOOLEAN,`if_class_wheel` BOOLEAN,`if_passage_wide` BOOLEAN,`if_door_handle` BOOLEAN,`door_handle_image` varchar(255),`if_knee_clear` BOOLEAN,`if_class_access_seats` BOOLEAN,`if_listen_device` BOOLEAN,`if_quiet_zone` BOOLEAN,`quiet_zone_image` varchar(255),`if_bath_sign` BOOLEAN,`if_faucets` BOOLEAN,`faucets_image` varchar(255))

CREATE TABLE IF NOT EXISTS `retail_reports` (`id` varchar(255) NOT NULL, `num_parking` INT, `num_handi_parking` INT, `handi_park_image` varchar(255),`if_handi_stripes` BOOLEAN,`if_multi` BOOLEAN,`if_elevator` BOOLEAN,`if_elevator_wide` BOOLEAN,`if_ramp` BOOLEAN,`if_ramp_wide` BOOLEAN,`if_60_entrances` BOOLEAN,`if_display_cases_wide` BOOLEAN,`if_checkout_high` BOOLEAN,`if_changing_cubicles` BOOLEAN,`if_quiet_zone`BOOLEAN,`quiet_zone_image` varchar(255),`if_bench` BOOLEAN,`if_bench_high` BOOLEAN,`if_door_handle` BOOLEAN,`door_handle_image` varchar(255),`if_food_court` BOOLEAN,`if_5_seating` BOOLEAN,`if_bath_sign` BOOLEAN,`if_toilets_wide` BOOLEAN,`if_room_in_stall` BOOLEAN,`if_faucets` BOOLEAN,`faucets_image` varchar(255))

CREATE TABLE IF NOT EXISTS `hotel_reports` (`id` varchar(255) NOT NULL, `num_parking` INT, `num_handi_parking` INT, `handi_park_image` varchar(255),`if_handi_stripes` BOOLEAN,`handi_stripes_image` varchar(255),`if_multi` BOOLEAN,`if_elevator` BOOLEAN,`if_elevator_wide` BOOLEAN,`if_ramp` BOOLEAN,`if_ramp_wide` BOOLEAN,`if_min_shelf_clear` BOOLEAN,`if_counters_34` BOOLEAN,`if_accessible_route` BOOLEAN,`if_quiet_zone`BOOLEAN,`quiet_zone_image` varchar(255),`total_guest_rooms` INT, `guest_rooms_rollin` INT, `guest_rooms_comms` INT, `if_wheel_turn` BOOLEAN, `if_clear_floor` BOOLEAN, `if_door_passage` BOOLEAN,`if_door_handle` BOOLEAN,`door_handle_image` varchar(255),`if_bath_sign` BOOLEAN,`if_toilets_wide` BOOLEAN,`if_room_in_stall` BOOLEAN,`if_faucets` BOOLEAN,`faucets_image` varchar(255))

ALTER TABLE school_reports ADD COLUMN handi_stripes_image VARCHAR(255) AFTER if_handi_stripes;

